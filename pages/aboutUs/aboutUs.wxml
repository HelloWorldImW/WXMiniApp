<!--pages/aboutUs/aboutUs.wxml-->

<!--通用标题栏  -->
<template name="titleLable">
  <view class='titleLabel' bindtap='clickMap'>
    <image class='titleLabel_leftIcon' src="/resource/location.png"></image>
    <text class='titleLabel_title'>{{title}}</text>
    <image class='titleLabel_rightIcon' src="/resource/gotoNext.png"></image>
  </view>
</template>
